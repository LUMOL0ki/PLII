<Window x:Class="PLII.cv5.UI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PLII.cv5.UI"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Margin="24,24,24,0">
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <local:ListControl x:Name="QueueListControl" HorizontalAlignment="Stretch" Margin="0,0,12,0" VerticalAlignment="Stretch"/>
        <local:ListControl x:Name="StackListControl" Grid.Column="1" HorizontalAlignment="Stretch" Margin="12,0,0,0" VerticalAlignment="Stretch"/>

    </Grid>
</Window>
